{
  "name": "upload",
  "version": "1.0.0",
  "description": "The upload server for Confrer",
  "main": "src/index.js",
  "scripts": {
    "start": "node src/index.js",
    "start:dev": "nodemon src/index.js",
    "test": "mocha src/**/*.test.js",
    "lint": "npm run lint:js",
    "lint:eslint": "eslint --ignore-path .gitignore",
    "lint:js": "npm run lint:eslint -- . ",
    "lint:prettier": "prettier --trailing-comma all --write \"{src,migrations}/**/*.js\"",
    "lint:staged": "lint-staged"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/Confrer/upload.git"
  },
  "engines": {
    "npm": ">=5",
    "node": ">=8"
  },
  "author": "Confrere AS",
  "license": "MIT",
  "private": true,
  "lint-staged": {
    "*.js": [
      "lint:prettier",
      "git add",
      "lint:eslint"
    ]
  },
  "pre-commit": "lint:staged",
  "eslintConfig": {
    "extends": [
      "airbnb-base",
      "prettier"
    ],
    "env": {
      "es6": true,
      "node": true,
      "mocha": true
    },
    "plugins": [
      "prettier",
      "mocha"
    ],
    "parserOptions": {
      "ecmaVersion": 2017,
      "sourceType": "script"
    },
    "rules": {
      "prettier/prettier": [
        "error",
        {
          "trailingComma": "all"
        }
      ],
      "no-console": 2
    }
  },
  "dependencies": {
    "aws-sdk": "^2.179.0",
    "body-parser": "^1.18.0",
    "config": "^1.26.1",
    "cors": "^2.8.4",
    "express": "^4.16.2",
    "express-async-await": "^1.1.0",
    "express-winston": "^2.4.0",
    "form-data": "^2.3.1",
    "fs-readfile-promise": "^3.0.1",
    "multer": "^1.3.0",
    "sharp": "^0.18.4",
    "uuid": "^3.1.0",
    "winston": "^2.3.1"
  },
  "devDependencies": {
    "axios": "^0.17.1",
    "eslint": "^4.3.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-config-prettier": "^2.3.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-mocha": "^4.11.0",
    "eslint-plugin-prettier": "^2.1.2",
    "lint-staged": "^6.0.0",
    "mocha": "^4.1.0",
    "nodemon": "^1.11.0",
    "pre-commit": "^1.2.2",
    "prettier": "^1.7.4"
  },
  "false": {}
}
